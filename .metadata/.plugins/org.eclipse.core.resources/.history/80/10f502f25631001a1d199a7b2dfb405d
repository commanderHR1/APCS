import java.util.Scanner;

public class LabProgram_ExactChange {
	//Gets exact change
	public static void exactChange(int userTotal, int[] coinVals) {
		int remainingTotal = userTotal;

		if (remainingTotal <= 0) {
			System.out.println("no change");
		}
		else {
			if (remainingTotal < 5) {
				coinVals[4] = remainingTotal;
			}
			else if (remainingTotal < 10) {
				coinVals[3] = 1;
				remainingTotal -= 5;
				coinVals[4] = remainingTotal;
			}
			else if (remainingTotal < 25) {

			}
		}
	}

	//MAIN
	public static void main(String[] args) {
		Scanner scnr = new Scanner(System.in);
		int input = scnr.nextInt();
		int[] coinVals = new int[5];


	}

}
